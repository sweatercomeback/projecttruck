<h1>New project_step</h1>

<%= error_messages_for :project_step %>

<% form_for(@project_step) do |f| %>
  <p>
    <b>Project</b><br />
    <%= f.text_field :project_id %>
  </p>

  <p>
    <b>Details</b><br />
    <%= f.text_area :details %>
  </p>

  <p>
    <b>Photo</b><br />
    <%= f.text_field :photo_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', project_steps_path %>
